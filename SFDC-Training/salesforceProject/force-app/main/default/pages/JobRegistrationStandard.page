<apex:page standardController="Job_Priyanka__c" standardStylesheets="false">
  <apex:includeScript value="{!URLFOR($Resource.StyleToVF,'Style_to_vf/JSForjob/scriptForjob.js')}"/>
        
<apex:Form >
<apex:pageBlock title="Job" mode="edit">
<apex:pageBlockButtons >
<apex:commandButton value="save" action="{!save}" onclick="return validate();"/>
</apex:pageBlockButtons>
<apex:pageBlockSection title="My job section" columns="1">
<apex:inputField value="{!Job_Priyanka__c.Active__c}"/>
<apex:inputField value="{!Job_Priyanka__c.Manager__c}" id="manager"/>

<apex:inputField value="{!Job_Priyanka__c.Expires_On__c}"/>
<apex:inputField value="{!Job_Priyanka__c.Hired_Applicants__c}"/>
<apex:inputField value="{!Job_Priyanka__c.Job_Types__c}" id="type"/>
<apex:inputField value="{!Job_Priyanka__c.Number_of_Positions__c}" id="No_Of_Pos"/>
<apex:inputField value="{!Job_Priyanka__c.Qualification_Required__c}"/>
<apex:inputField value="{!Job_Priyanka__c.Required_Skill__c}"/>
<apex:inputField value="{!Job_Priyanka__c.Certification_Required__c}"/>
<apex:inputField value="{!Job_Priyanka__c.Salary_Offered__c}"/>
<apex:inputField value="{!Job_Priyanka__c.Total_Applicants__c}"/>
    <apex:inputField value="{!Job_Priyanka__c.Description__c}"/>



</apex:pageBlockSection>

</apex:pageBlock>
</apex:Form>

 </apex:page>